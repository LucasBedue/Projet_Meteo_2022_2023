set rrange [ * : * ] noreverse writeback

set tics nomirror
set xtics("07005" 1,"07015" 2,"07020" 3,"07027" 4,"07037" 5,"07072" 6,"07110" 7,"07117" 8,"07130" 9,"07139" 10,"07149" 11,"07168" 12,"07181" 13,"07190" 14,"07207" 15,"07222" 16,"07240" 17,"07255" 18,"07280" 19,"07299" 20,"07314" 21,"07335" 22,"07434" 23,"07460" 24,"07471" 25,"07481" 26,"07510" 27,"07535" 28,"07558" 29,"07577" 30,"07591" 31,"07607" 32,"07621" 33, "07627" 34,"07630" 35,"07643" 36,"07650" 37,"07661" 38,"07690" 39,"07747" 40,"07761" 41,"07790" 42,"61968" 43,"61970" 44,"61972" 45,"61976" 46,"61980" 47,"61996" 48,"61997" 49,"61998" 50,"67005" 51,"71805" 52,"78890" 53,"78894" 54,"78897" 55,"78922" 56,"78925" 57,"81401" 58,"81405" 59,"81408" 60,"81415" 61,"89642" 62)

